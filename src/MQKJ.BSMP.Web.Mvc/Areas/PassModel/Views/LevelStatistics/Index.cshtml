
@using MQKJ.BSMP.Web.Startup

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section styles{
    <link href="~/lib/laydate/theme/default/laydate.css" asp-append-version="true" />
}
@section scripts
    {
    <script src="~/lib/laydate/laydate.js" asp-append-version="true"></script>
    <script src="~/view-resources/Views/PassModel/level.js"></script>
}
@{
    ViewBag.CurrentPageName = PageNames.PassModel; // The menu item will be active for this page.
}

    <div class="row clearfix">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="card">
                <div class="header">
                    <h2>
                        关卡分布
                    </h2>
                    <ul class="header-dropdown m-r--5">
                        <li class="dropdown">
                            <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                <i class="material-icons">more_vert</i>
                            </a>
                            <ul class="dropdown-menu pull-right">
                                <li><a id="RefreshButton" href="javascript:void(0);" class="waves-effect waves-block"><i class="material-icons">refresh</i>刷新</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <div class="row clearfix" style="margin-left:10px;margin-top:10px;" id="searchform">
                    <form role="form" novalidate class="form-validation">
                        <div class="col-sm-11">
                            <div class="form-group user-style">
                                <input id="dateTime" type="text" value="" name="dateTime" readonly="readonly" class="form-inline">
                                <button style="margin-left:20px;" type="button" class="btn btn-primary btn-circle waves-effect waves-circle waves-float " id="searchBtn">
                                    <i class="material-icons">search</i>
                                </button>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="body table-responsive" id="dataList">
                    @Html.Partial("_List")
                </div>
            </div>
        </div>
    </div>